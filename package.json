{
  "name": "@usace/groundwork",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/groundwork.umd.cjs",
  "module": "./dist/groundwork.js",
  "exports": {
    ".": {
      "import": "./dist/groundwork.js",
      "require": "./dist/groundwork.umd.cjs"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build-lib && npm run build-docs",
    "build-lib": "vite build -m lib",
    "build-docs": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pub": "npm run build-lib && npm publish --access=public",
    "major": "npm version major && npm run pub",
    "minor": "npm version minor && npm run pub",
    "patch": "npm version patch && npm run pub"
  },
  "dependencies": {
    "feather-route-matcher": "^4.0.0",
    "internal-nav-helper": "^3.1.0",
    "react-icons": "^4.12.0",
    "styled-components": "^6.1.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
