{
  "ci": {
    "collect": {
      "url": ["http://localhost:4173/"],
      "startServerCommand": "cd src && npm run build && npm run preview -- --port 4173 --strictPort",
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./.lighthouseci"
    }
  }
}
